[ignore]
<PROJECT_ROOT>/git/.*
<PROJECT_ROOT>/node_modules/*
<PROJECT_ROOT>/src/registerServiceWorker.js
<PROJECT_ROOT>/src/public/.*
<PROJECT_ROOT>/src/scripts/.*
<PROJECT_ROOT>/release/.*

[include]

[libs]
<PROJECT_ROOT>/flow-typed

[lints]

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/flow-typed/CSSModule.js.flow'
module.name_mapper.extension='styl' -> '<PROJECT_ROOT>/flow-typed/CSSModule.js.flow'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/flow-typed/CSSModule.js.flow'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/flow-typed/WebpackAsset.js.flow'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/flow-typed/WebpackAsset.js.flow'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
